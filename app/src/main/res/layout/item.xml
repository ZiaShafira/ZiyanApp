<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Inventory"
    android:background="@color/aliceblue">

    <TextView
        android:id="@+id/txt_namabarang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_toEndOf="@+id/imageView5"
        android:text="NamaBarang"
        android:textSize="18sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/txt_merkbarang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/txt_namabarang"
        android:layout_toEndOf="@+id/imageView5"
        android:text="MerkBarang"
        android:ellipsize="end"
        android:textSize="18sp"/>

    <TextView
        android:id="@+id/txt_jenisbarang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/txt_merkbarang"
        android:layout_toEndOf="@+id/imageView5"
        android:text="JenisBarang"
        android:ellipsize="end"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txt_stok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_harga"
        android:layout_toEndOf="@+id/imageView5"
        android:layout_marginStart="26dp"
        android:layout_marginTop="5dp"
        android:ellipsize="end"
        android:text="Stok"
        android:textSize="18sp" />
    <TextView
        android:id="@+id/txt_harga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_jenisbarang"
        android:layout_toEndOf="@+id/imageView5"
        android:layout_marginStart="26dp"
        android:layout_marginTop="5dp"
        android:ellipsize="end"
        android:text="Harga"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="121dp"
        android:layout_height="133dp"
        app:srcCompat="@drawable/ic_baseline_arrow_forward_24" />

</RelativeLayout>